{
  "name": "timongqlserver",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "nodemon server.js --exec babel-node --presets es2015 --plugins transform-class-properties",
    "debug": "nodemon server.js --exec babel-node --presets es2015 --plugins transform-class-properties --inspect-brk",
    "flow": "flow",
    "test": "jest",
    "create-snaps-index": "babel-node ./elasticsearch/snapsMappings.js",
    "load-mock-snaps": "babel-node ./elasticsearch/load_snaps.js",
    "load-mock-snaps-sql": "babel-node ./elasticsearch/load_snaps_sql.js -t ES,mySQL"
  },
  "jest": {
    "collectCoverageFrom": [
      "*.{js,jsx}"
    ],
    "testEnvironment": "node",
    "transformIgnorePatterns": [
      "[/\\\\]node_modules[/\\\\].+\\.(js|jsx)$"
    ]
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "apollo-server": "^2.1.0",
    "apollo-server-express": "^2.1.0",
    "babel-core": "^6.26.3",
    "babel-polyfill": "^6.26.0",
    "casual": "^1.5.19",
    "express": "^4.16.3",
    "graphql": "^14.0.2",
    "lodash.assign": "^4.2.0",
    "moment-random": "^1.0.4",
    "mysql": "^2.16.0",
    "nodemon": "^1.17.3"
  },
  "devDependencies": {
    "babel-cli": "^6.26.0",
    "babel-plugin-transform-class-properties": "^6.24.1",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-flow": "^6.23.0",
    "babel-preset-stage-0": "^6.24.1",
    "jest": "^22.4.3"
  },
  "babel": {
    "presets": [
      "es2015",
      "flow",
      "stage-0"
    ],
    "plugins": [
      [
        "transform-class-properties",
        {
          "spec": true
        }
      ]
    ]
  }
}
